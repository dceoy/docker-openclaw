# Clawdbot Docker Configuration

# Clawdbot version to install (default: latest)
CLAWDBOT_VERSION=latest

# Node.js version (default: 22)
CLAWDBOT_NODE_VERSION=22

# Docker image name (default: clawdbot)
CLAWDBOT_IMAGE=clawdbot

# Configuration directory (default: ./.clawdbot)
CLAWDBOT_CONFIG_DIR=./.clawdbot

# Workspace directory (default: ./workspace)
CLAWDBOT_WORKSPACE_DIR=./workspace

# Gateway ports
CLAWDBOT_GATEWAY_PORT=18789
CLAWDBOT_GATEWAY_WS_PORT=18790

# Sandbox ports
CLAWDBOT_SANDBOX_PORT=18791
CLAWDBOT_SANDBOX_WS_PORT=18792

# Browser ports
CLAWDBOT_BROWSER_PORT=18793
CLAWDBOT_BROWSER_WS_PORT=18794

# Additional apt packages for sandbox image (space-separated)
CLAWDBOT_DOCKER_APT_PACKAGES=

# Claude credentials (required for Claude integration)
CLAUDE_AI_SESSION_KEY=
CLAUDE_WEB_SESSION_KEY=
CLAUDE_WEB_COOKIE=

# OpenAI API key (optional)
OPENAI_API_KEY=

# Anthropic API key (optional)
ANTHROPIC_API_KEY=

# ElevenLabs API key (optional, for voice features)
ELEVENLABS_API_KEY=
