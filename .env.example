# OpenClaw Docker Configuration

# OpenClaw version to install (default: latest)
OPENCLAW_VERSION=latest

# Node.js version (default: 22)
OPENCLAW_NODE_VERSION=22

# Docker image name (default: openclaw)
OPENCLAW_IMAGE=openclaw

# Configuration directory (default: ./.openclaw)
OPENCLAW_CONFIG_DIR=./.openclaw

# Workspace directory (default: ./workspace)
OPENCLAW_WORKSPACE_DIR=./workspace

# Gateway ports
OPENCLAW_GATEWAY_PORT=18789
OPENCLAW_GATEWAY_WS_PORT=18790

# Sandbox ports
OPENCLAW_SANDBOX_PORT=18791
OPENCLAW_SANDBOX_WS_PORT=18792

# Browser ports
OPENCLAW_BROWSER_PORT=18793
OPENCLAW_BROWSER_WS_PORT=18794

# Additional apt packages for sandbox image (space-separated)
OPENCLAW_DOCKER_APT_PACKAGES=

# Claude credentials (required for Claude integration)
CLAUDE_AI_SESSION_KEY=
CLAUDE_WEB_SESSION_KEY=
CLAUDE_WEB_COOKIE=

# OpenAI API key (optional)
OPENAI_API_KEY=

# Anthropic API key (optional)
ANTHROPIC_API_KEY=

# ElevenLabs API key (optional, for voice features)
ELEVENLABS_API_KEY=
